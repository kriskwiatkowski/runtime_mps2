{
    "version": 6,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 13,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "base",
            "generator": "Ninja",
            "hidden": true,
            "binaryDir": "${sourceDir}/out/build/${presetName}",
            "cacheVariables": {
                "CMAKE_INSTALL_PREFIX": "${sourceDir}/out/${presetName}"
            }
        },
        {
            "name": "an385",
            "displayName": "ARMv7-M Release",
            "inherits": "base",
            "toolchainFile": "toolchains/gcc-armv7_m-unknown-none-eabi.toolchain",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "FLAVOUR": "AN385"
            }
        },
        {
            "name": "an386",
            "inherits": "base",
            "displayName": "ARMv7e-M Release - AN386",
            "toolchainFile": "toolchains/gcc-armv7e_m-unknown-none-eabi.toolchain",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "EXTRA_C_FLAGS": "-DMPS2_ENABLE_FPU",
                "FLAVOUR": "AN386"
            }
        },
        {
            "name": "an500",
            "inherits": "an386",
            "displayName": "ARMv7e-M Release - AN500",
            "cacheVariables": {
                "FLAVOUR": "AN500"
            }
        },
        {
            "name": "an385-minsizerel",
            "inherits": "base",
            "displayName": "ARMv7-M Size optimized",
            "toolchainFile": "toolchains/gcc-armv7_m-unknown-none-eabi.toolchain",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "MinSizeRel",
                "FLAVOUR": "AN385"
            }
        },
        {
            "name": "an386-minsizerel",
            "inherits": "base",
            "displayName": "ARMv7e-M Size optimized",
            "toolchainFile": "toolchains/gcc-armv7e_m-unknown-none-eabi.toolchain",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "MinSizeRel",
                "EXTRA_C_FLAGS": "-DMPS2_ENABLE_FPU",
                "FLAVOUR": "AN386"
            }
        },
        {
            "name": "an500-minsizerel",
            "displayName": "ARMv7e-M Size optimized - AN500",
            "inherits": "an386-minsizerel",
            "cacheVariables": {
                "FLAVOUR": "AN500"
            }
        },
        {
            "name": "an385-debug",
            "inherits": "base",
            "displayName": "ARMv7-M RelWithDebInfo",
            "toolchainFile": "toolchains/gcc-armv7_m-unknown-none-eabi.toolchain",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "FLAVOUR": "AN385"
            }
        },
        {
            "name": "an386-debug",
            "inherits": "base",
            "displayName": "ARMv7e-M RelWithDebInfo",
            "toolchainFile": "toolchains/gcc-armv7e_m-unknown-none-eabi.toolchain",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "RelWithDebInfo",
                "EXTRA_C_FLAGS": "-DMPS2_ENABLE_FPU",
                "FLAVOUR": "AN386"
            }
        },
        {
            "name": "an500-debug",
            "inherits": "an386-debug",
            "displayName": "ARMv7e-M RelWithDebInfo - AN500",
            "cacheVariables": {
                "FLAVOUR": "AN500"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "base",
            "configurePreset": "an385",
            "hidden": true,
            "targets": [
                "all",
                "install"
            ]
        },
        {
            "name": "an385",
            "configurePreset": "an385",
            "inherits": "base"
        },
        {
            "name": "an386",
            "configurePreset": "an386",
            "inherits": "base"
        },
        {
            "name": "an500",
            "configurePreset": "an500",
            "inherits": "base"
        },
        {
            "name": "an385-minsizerel",
            "configurePreset": "an385-minsizerel",
            "inherits": "base"
        },
        {
            "name": "an386-minsizerel",
            "configurePreset": "an386-minsizerel",
            "inherits": "base"
        },
        {
            "name": "an500-minsizerel",
            "configurePreset": "an500-minsizerel",
            "inherits": "base"
        },
        {
            "name": "an385-debug",
            "configurePreset": "an385-debug",
            "inherits": "base"
        },
        {
            "name": "an386-debug",
            "configurePreset": "an386-debug",
            "inherits": "base"
        },
        {
            "name": "an500-debug",
            "configurePreset": "an500-debug",
            "inherits": "base"
        }
    ],
    "packagePresets": [
        {
            "name": "an385",
            "configurePreset": "an385",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        },
        {
            "name": "an386",
            "configurePreset": "an386",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        },
        {
            "name": "an500",
            "configurePreset": "an500",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        },
        {
            "name": "an385-minsizerel",
            "configurePreset": "an385-minsizerel",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        },
        {
            "name": "an386-minsizerel",
            "configurePreset": "an386-minsizerel",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        },
        {
            "name": "an500-minsizerel",
            "configurePreset": "an500-minsizerel",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        },
        {
            "name": "an385-debug",
            "configurePreset": "an385-debug",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        },
        {
            "name": "an386-debug",
            "configurePreset": "an386-debug",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        },
        {
            "name": "an500-debug",
            "configurePreset": "an500-debug",
            "packageDirectory": "${sourceDir}/out/package/${presetName}"
        }
    ]
}